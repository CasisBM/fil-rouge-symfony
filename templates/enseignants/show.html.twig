{% extends 'base.html.twig' %}

{% block title %}Enseignants{% endblock %}

{% block body %}
    <h1>Enseignants</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enseignant.id }}</td>
            </tr>
            <tr>
                <th>Identifiant</th>
                <td>{{ enseignant.identifiant }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ enseignant.password }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ enseignant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ enseignant.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ enseignant.email }}</td>
            </tr>
            <tr>
                <th>Date_naissance</th>
                <td>{{ enseignant.dateNaissance ? enseignant.dateNaissance|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ enseignant.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_enseignants_index') }}">back to list</a>

    <a href="{{ path('app_enseignants_edit', {'id': enseignant.id}) }}">edit</a>

    {{ include('enseignants/_delete_form.html.twig') }}
{% endblock %}
